/var/log/cron
/var/log/maillog
{
    missingok
    dateext
    daily
    compress
    rotate 30
    start 0
    create
    lastaction
        /home/ssm-user/maillog-rotate.sh
    endscript
}
/var/log/messagesgit 
/var/log/secure
/var/log/spooler
{
    missingok
    sharedscripts
    postrotate
	/bin/kill -HUP `cat /var/run/syslogd.pid 2> /dev/null` 2> /dev/null || true
    endscript
}
